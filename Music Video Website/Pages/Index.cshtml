@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<div class="text-center">
    <h1 class="display-4">Stick Figure Music</h1>
    <p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
</div>

<!DOCTYPE html>
<html>
<head>
<style>
#container {
    width: 400px;
    height: 400px;
    position: relative;
    background: purple;
}
#animate {
    width: 50px;
    height: 50px;
    position: absolute;
    background-color: green;
}
body {
    background-color: lightblue;
}
</style>
</head>
<body>

<p><button onclick="myMove()">Click It!</button></p>

<div id="container">
    <div id="animate"></div>
</div>

<script>
    function myMove() {
        let id = null;
        const elem = document.getElementById("animate");
        let pos = 0;
        clearInterval(id);
        id = setInterval(frame, 5);
        function frame() {
            if (pos == 350) {
                clearInterval(id);
            } else {
                pos++;
                elem.style.top = pos + "px";
                elem.style.left = pos + "px";
            }
        }
    }
</script>

<iframe width="400" height="245" src="https://www.youtube.com/embed/OHq3-cqCogc">
</iframe>

</body>
</html>